<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="heartData">

    <insert id="insert" parameterType="heartData">
        <![CDATA[
                INSERT INTO heart_data (ip, device_id ,state_code )
                 VALUES (#{ip},#{deviceId},#{stateCode})
        ]]>
    </insert>
</mapper>
